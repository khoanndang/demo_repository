    "paths": {
        "/demo_repository/public/api/product/find-by-status": {
            "get": {
                "tags": [
                    "PRODUCT"
                ],
                "summary": "Finds products by status",
                "description": "API get products by status",
                "operationId": "findByStatus",
                "parameters": [
                    {
                        "name": "status",
                        "in": "query",
                        "description": "Status values that needed to be considered for filter",
                        "required": true,
                        "explode": true,
                        "schema": {
                            "type": "string",
                            "default": "available",
                            "enum": [
                                "available",
                                "pending",
                                "sold"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid status value"
                    }
                }
            }
        },
        "/demo_repository/public/api/product/test-version": {
            "get": {
                "tags": [
                    "PRODUCT"
                ],
                "summary": "Test version",
                "description": "Test version",
                "operationId": "testVersion",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid status value"
                    }
                }
            }
        }
    }
}